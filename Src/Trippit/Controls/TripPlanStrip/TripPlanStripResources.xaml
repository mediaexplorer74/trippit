<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Trippit.Controls">

    <x:Double x:Key="TripPlanStripRectangleHeight">5</x:Double>
    <x:Double x:Key="TripPlanStripCircleHeight">12</x:Double>
    <x:Double x:Key="TripPlanStripCircleWidth">12</x:Double>

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="TripPlanStripRectangleBrush" Color="#FF7B7B7B"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="TripPlanStripRectangleBrush" Color="#FF7B7B7B"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    
</ResourceDictionary>
